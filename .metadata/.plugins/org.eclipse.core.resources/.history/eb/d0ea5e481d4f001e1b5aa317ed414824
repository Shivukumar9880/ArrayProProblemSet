package Array_problems;

public class _1_reverrse_Array 
{

	public static void main(String[] args) {
		
		int a[]= {10,20,30,40,50};
		
		int j=a.length-1;
//		int n=j;
		int i=0;
	for(;i<=a.length-1;i++)
		{
		   int x=a[i];
		   a[i]=a[j];
		   a[j]=x;
		   System.out.println(j);
		   j--;
		  
		}
	
		for(int x=0;x<a.length;x++)
		{
			System.out.println(a[x]);
		}
	}
}
