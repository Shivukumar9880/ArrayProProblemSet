package Array_Searching_And_Sorting_problrms;

public class pow {

	public static void main(String[] args) {
		
		long prod=1;
		
		int i=1;
		int n=63;
		long pow=2; 
		while(i<n)
		{
//			prod=prod*pow;
			pow*=2;
			i++;
			
		}
		System.out.println(pow-1);
		
		
		float f=10.9f;
		double d=80;
		
	}
}
