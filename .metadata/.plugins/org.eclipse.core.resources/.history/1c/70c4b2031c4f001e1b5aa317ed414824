package Array_problems;

public class _1_reverrse_Array 
{

	public static void main(String[] args) {
		
		int a[]= {10,20,30,40,50};
		
		int j=a.length-1;
		for(int i=0;i<a.length;i++)
		{
		   int x=a[i];
		   a[i]=a[j];
		   a[j]=x;
		   j--;
		}
		for(int i=0;i<a.length;i++)
		{
			System.out.println(a[i]);
		}
	}
}
